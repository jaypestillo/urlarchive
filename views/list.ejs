<h3>URL's</h3>
<input type="text" class="form-control search" id="search" placeholder="Type to search">
<div class="container">
<table class="table table-striped table-hover table-sm" id="dataTable">
  <thead class="thead">
    <tr>
      <!-- <th>#</th> -->
      <th align="center" >title</th>
      <th align="center" >category</th>
      <th align="center" >date</th>
      <th align="center" >URL</th>
      <th></th>
    </tr>
  </thead>
  <tfoot>
    <tr>
      <th align="center" >title</th>
      <th align="center" >category</th>
      <th align="center" >date</th>
      <th align="center" >URL</th>
      <th></th>
    </tr>
  <tbody>
    <% entries.forEach(function(entry) { %>
      <tr>
        <!-- <td> <%= entry.id %> </td> -->
        <td><strong><%= entry.title %></strong></td>
        <td><%= entry.category %></td>
        <td><%= entry.date %></td>
        <td><em><%= entry.url %></em></td>
        <td class="right"><a href="/entries/edit/<%= entry.id %>" class="btn btn-secondary">Update</a></td>
      </tr>
    <% }) %>
  </tbody>
</table>
</div>
