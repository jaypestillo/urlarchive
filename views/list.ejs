<div class="fixedSearchBar">
<h3>Archive</h3>
<input type="text" class="form-control search" id="search" placeholder="Type to search">
</div>
<div class="scrollable">
<table class="table table-hover table-sm" id="dataTable">
  <thead class="thead">
    <tr>
      <!-- <th>#</th> -->
      <th width="320px" align="center" >title</th>
      <th width="320px" align="center" >category</th>
      <th width="170px" align="center" >date</th>
      <th width="170px" align="center" >URL</th>
      <th width="120px" ></th>
    </tr>
  </thead>
  <!-- <tfoot> -->
  <!-- <tr> -->
  <!-- <th width="320px" align="center" >title</th> -->
  <!-- <th width="320px" align="center" >category</th> -->
  <!-- <th width="170px" align="center" >date</th> -->
  <!-- <th width="170px" align="center" >URL</th> -->
  <!-- <th width="120px" ></th> -->
  <!-- </tr> -->
  <!-- </tfoot> -->
  <!-- <tbody> -->
    <% entries.forEach(function(entry) { %>
        <tr>
          <td width="320px"><strong><%= entry.title %></strong></td>
          <td width="320px"><%= entry.category %></td>
          <td width="170px"><%= entry.date %></td>
          <td width="170px"><em><a href="<%= entry.url %>" target="_blank">URL Link</a></em></td>
          <td width="120px" class="right"><a href="/entries/edit/<%= entry.id %>" class="btn btn-secondary">Update</a></td>
      </tr>
    <% }) %>
  </tbody>
</table>
</div>
